{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...]"
    },
    {
      "source": "/product$",
      "destination": "/public/product.html"
    },
    {
      "source": "/payment-complete$",
      "destination": "/public/payment-complete.html"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ]
}
